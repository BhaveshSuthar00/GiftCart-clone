    <div id="head">
        <h2 >HATKE GIFTS</h2>
        <div id="line"></div>
        <h3 >
            <a href="/product/random?item=Personalised Gift">View More</a>
        </h3>
    </div>
    <div id="Bs_container_div">
        <% personalisedGift.forEach(function(element) { %>
            <div class="Bs_item_div">
                <div class="Bs_image_div">
                    <a class="Bs_atag_Product" href="/shopitem/<%- element._id %>">
                    <img src="<%= element.image %>" alt="">
                    </a>
                    <div class="Bs_slide_p">
                        <div>
                                <a href="/product/random?item=<%- element.category %>&sub=<%- element.sub_category %>">
                                    Similar Gifts
                                </a>
                        </div>
                    </div>
                </div>
                <div class="Bs_text_div">
                    <div class="Bs_text_div_1">
                        <p>
                            <%= element.product %>
                        </p>
                        <p>
                            <i class="fas fa-heart"></i>
                        </p>
                    </div>
                    <div class="Bs_text_div_2">
                        <p>MRP(incl gst)</p>
                        <p>₹<%= element.price %>.00</p>
                    </div>
                </div>
                <div class="Bs_star_image_div">
                    <i class="fas fa-star"></i>
                    <i class="fas fa-star"></i>
                    <i class="fas fa-star"></i>
                    <i class="fas fa-star"></i>
                    <i class="far fa-star"></i>
                </div>
            </div>
        <% }) %>

